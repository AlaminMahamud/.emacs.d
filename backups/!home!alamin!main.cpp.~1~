#include <bits/stdc++.h>
using namespace std;
int R[100];

void rotate(int arr[], int n){
  int M=0; 
  for(int i = 0; i < n; i++){
    R[M] += (((i+M)%n)*arr[i]);
    R[M+1] += (((i+M+1)%n)*arr[i]);
    R[M+2] += (((i+M+2)%n)*arr[i]);
    R[M+3] += (((i+M+3)%n)*arr[i]);
    R[M+4] += (((i+M+4)%n)*arr[i]);    
  }
}

void max_sum(int n){
  int max = -1;
  for(int i = 0; i < n; i++){
    if(max < R[i]) max = R[i];
  }
  cout << max << endl;
}


int main(){
  int arr[] = {5,4,3,2,1};
  rotate(arr, 5);
  max_sum(5);
  return 0;
}

